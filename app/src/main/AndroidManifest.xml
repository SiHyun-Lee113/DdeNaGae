<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Dde_Na_Gae">

    // 인터넷 권한 설정
    <uses-permission android:name="android.permission.INTERNET"/>
    // 위치 서비스 권한 설정
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Login_Example"
        android:usesCleartextTraffic="true">

        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>

                <data android:scheme="@string/kakao_scheme"
                    android:host="@string/kakaolink_host"/>
            </intent-filter>
        </activity>
        <activity android:name=".Alarm" />
        <activity android:name="com.example.Dde_Na_Gae.BestPlace" />
        <activity android:name="com.example.Dde_Na_Gae.BestWalk" />
        <activity android:name="com.example.Dde_Na_Gae.Category" />
        <activity android:name="com.example.Dde_Na_Gae.Login_New_Page"/>
        <activity android:name="com.example.Dde_Na_Gae.LoginPage" />
        <activity android:name="com.example.Dde_Na_Gae.Mainactivity"/>
        <activity android:name="com.example.Dde_Na_Gae.Matching" />
        <activity android:name=".Matching_Option"/>
        <activity android:name=".My_Page" />
        <activity android:name=".Notice" />
        <activity android:name=".Personal_information" />
        <activity android:name=".Search" />
        <activity android:name=".Search_Selected" />
        <activity android:name=".Search_Selected_More" />
        <activity android:name=".Setting" />
        <activity android:name="com.example.Dde_Na_Gae.Sign_up" />
        <activity android:name=".Terms_of_service" />

        <meta-data
            android:name="com.kakao.sdk.Appkey"
            android:value="@string/kakao_app_key"/>

    </application>

</manifest>