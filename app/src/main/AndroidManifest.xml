<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Dde_Na_Gae">

    <!-- 인터넷 사용 권한 설정-->
   <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Login_Example"
        android:name=".KakaoApplication"
        >
        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="b214210d275c3c68a2ab5a59099d8b41"/>

        <activity android:name="com.example.Dde_Na_Gae.Mainactivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>

            <activity android:name="com.kakao.sdk.auth.AuthCodeHandlerActivity">
                <intent-filter>
                    <action android:name="android.intent.action.VIEW" />
                    <category android:name="android.intent.category.DEFAULT" />
                    <category android:name="android.intent.category.BROWSABLE" />

                    <!-- Redirect URI: "kakao{NATIVE_APP_KEY}://oauth" -->
                    <data android:host="oauth"
                        android:scheme="kakaob214210d275c3c68a2ab5a59099d8b41" />
                </intent-filter>
            </activity>

        <activity android:name="com.example.Dde_Na_Gae.Sign_up" />
        <activity android:name="com.example.Dde_Na_Gae.BestPlace" />
        <activity android:name="com.example.Dde_Na_Gae.BestWalk" />
        <activity android:name="com.example.Dde_Na_Gae.Category" />
        <activity android:name="com.example.Dde_Na_Gae.TodayPlace" />
        <activity android:name="com.example.Dde_Na_Gae.Matching" />
        <activity android:name="com.example.Dde_Na_Gae.LoginPage" />
        <activity android:name="com.example.Dde_Na_Gae.Login_New_Page"/>
        <activity android:name=".My_Information"/>
        <activity android:name=".My_Page"/>
    </application>

</manifest>